{
    "name": "Py Hello World",
    "version": "1.0",
    "description": "Hello World Chrome Extension in Python",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "popup.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval';object-src 'self'",
    "permissions": ["declarativeContent", "storage", "activeTab"]
}
